{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "run-s clean public:build svelte:build",
    "dev": "run-p public:dev svelte:dev",
    "start": "sirv dist",
    "clean": "rimraf dist storybook-static",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint './src/**/*.{js,ts,svelte}'",
    "format": "prettier . --write",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "public:dev": "gulp watchPublic",
    "public:build": "gulp buildPublic",
    "svelte:dev": "rollup -c -w",
    "svelte:build": "rollup -c",
    "svelte:check": "svelte-check"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@storybook/addon-actions": "^6.2.0-rc.4",
    "@storybook/addon-essentials": "^6.2.0-rc.4",
    "@storybook/addon-links": "^6.2.0-rc.4",
    "@storybook/svelte": "^6.2.0-rc.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.0",
    "@types/gulp": "^4.0.8",
    "@types/gulp-clean-css": "^4.3.0",
    "@types/gulp-filter": "^3.0.33",
    "@types/gulp-htmlmin": "^1.3.32",
    "@types/gulp-jsonminify": "^1.0.0",
    "@types/gulp-sourcemaps": "^0.0.34",
    "@types/gulp-terser": "^1.2.1",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "eslint": "^7.27.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-filter": "^6.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-jsonminify": "^1.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.0.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "~2.3.0",
    "prettier-plugin-svelte": "~2.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.34.1",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-jester": "^1.3.0",
    "svelte-loader": "^3.0.0",
    "svelte-preprocess": "^4.0.0",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
